{
  "name": "console_font",
  "version": "1.0.5",
  "description": "A package to have somehow some nice 'font' in console.log ! Yep, you get it, some figlet like getting in your js console",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "license": "MIT",
  "author": "Gabriel Somogyi",
  "type": "module",
  "keywords": [
    "console",
    "logger",
    "figlet",
    "ascii-art"
  ],
  "scripts": {
    "dev": "vite serve src/examples",
    "build": "vite build --mode library",
    "build:examples": "vite build --emptyOutDir --outDir dist-examples",
    "preview": "vite preview --outDir dist-examples",
    "test": "echo \"Run examples to test functionality\""
  },
  "devDependencies": {
    "vite": "^6.3.5"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./LoggerLikeBulbhead": {
      "import": "./dist/components/LoggerLikeBulbhead/Logger.js"
    },
    "./LoggerLikeLarry3D": {
      "import": "./dist/components/LoggerLikeLarry3D/Logger.js"
    }
  }
}
